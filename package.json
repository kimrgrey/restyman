{
  "name": "restyman",
  "version": "1.1.0",
  "description": "REST API client",
  "bugs": {
    "url": "https://github.com/kimrgrey/restyman/issues"
  },
  "homepage": "https://github.com/kimrgrey/restyman",
  "repository": {
    "url": "https://github.com/kimrgrey/restyman.git",
    "type": "git"
  },
  "keywords": [
    "rest",
    "axios",
    "client",
    "api",
    "json"
  ],
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/restyman.js",
  "scripts": {
    "build": "npm run clean && mkdir dist && npm-run-all --parallel build:development build:production",
    "build:development": "NODE_ENV=development rollup -c",
    "build:production": "NODE_ENV=production rollup -c",
    "clean": "rm -rf dist",
    "lint": "eslint ./ --fix",
    "prebuild": "npm run clean",
    "prepublish": "npm run build"
  },
  "author": "Sergey Tsvetkov",
  "contributors": [
    "Sergey Makarov"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {}
}
